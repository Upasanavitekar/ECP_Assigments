#include<stdio.h>
int main()
{
	int num,rev,rem;
	printf("enter the number");
	scanf("%d",&num);
	int ori = num;
        while(num !=0)
	{
		rem = num%10;
		rev = rev*10 + num;
		num = num/10;
		
	}
	if (rev == ori)
	{
   		printf("number is palindrome");
	}
	else
	{
		printf("number is not palindrome");
	}	
	return 0;
}
